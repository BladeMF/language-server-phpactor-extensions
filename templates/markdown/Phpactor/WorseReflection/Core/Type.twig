{%- if object.nullable %}?{% endif -%}
{%- if not object.arrayType.isDefined -%}
    {{ object.short }}
{%- else -%}
    {%- if object.isClass %}
        {{- object.short }}<{{ object.arrayType.short }}> 
    {%- elseif object.arrayType.isDefined -%}
        array<{{- object.arrayType -}}>
    {%- else -%}
        {{ object.short }}
    {%- endif -%}
{%- endif -%}
